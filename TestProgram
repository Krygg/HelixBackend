bpm(60);

num x = 3;
num m = 69+x;
num y = 10;

notes melody = c4,c5,d8;

start p;

Piano p {

    x = x + 1;

    mel(melody);

    time(3,5);

    adsr(2,3,4,5);

    send(c,7+4);

    start d;

}

Drum d {

    receive(c,h) . send(i, h);

    start q;


}

Piano q {

    receive(i,l).;
}

Drum k {

    start q;

}

Piano pi{

    mel(melody);

}