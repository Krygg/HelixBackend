bpm(60);

num x = 3;
num m = 69+x;
num y = 10;

notes melody = c4,c5,d8;

start p;

Piano p {

    x = x + 1;

    mel(melody);

    time(3,5);

    adsr(2,3,4,5);

    send(c, 7);

    start d;

}

Drum d {

    receive(c,h).y = h;
    adsr(2,3,4,5);

    start pi;
}

Piano pi{

    mel(melody);

}